{
  "name": "gimmenetwork/simplon_form",
  "description": "[FORK] Data validation & flexible form rendering incl. semantic-ui structure.",
  "homepage": "https://github.com/fightbulc/simplon_form",
  "type": "library",
  "keywords": [
    "form",
    "fields",
    "validation",
    "rules",
    "filters",
    "templates",
    "phtml",
    "semantic-ui",
    "calendar"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tino Ehrich",
      "email": "tino@bigpun.me",
      "role": "developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_phtml"
    },    {
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_helper"
    },    {
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_component_form"
    },    {
      "type": "vcs",
      "url": "https://github.com/gimmenetwork/simplon_component_semanticui"
    }
  ],
  "require": {
    "php": ">=7.1",
    "gimmenetwork/simplon_phtml": "^0.2",
    "gimmenetwork/simplon_helper": "^1.0",
    "fightbulc/moment": "^1.26",
    "gimmenetwork/simplon_component_form": "^0.2",
    "gimmenetwork/simplon_component_semanticui": "^0.0"
  },
  "config": {
    "component-dir": "assets/vendor"
  },
  "autoload": {
    "psr-4": {
      "Simplon\\Form\\": "src/"
    }
  },
  "replace": {
    "simplon/form": "2.5.*"
  }
}
